{
    "cli": {
        "version": ">= 12.4.1",
        "appVersionSource": "remote"
    },
    "build": {
      "development": {
        "distribution": "internal",
        "android": {
          "buildType": "apk"
        },
        "ios": {
          "simulator": true
        }
      },
      "preview": {
        "ios":{
          "simulator":true
        },
        "android":{
          "buildType":"apk"
        }
      },
      "production": {
        "distribution": "store",
        "android": {
          "buildType": "app-bundle",
          "autoIncrement": "versionCode"
        },
        "ios": {
          "enterpriseProvisioning": "universal"
        }
      }
    }
  }
  